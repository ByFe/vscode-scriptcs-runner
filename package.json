{
	"name": "ScriptCsRepl",
	"description": "REPL for scriptcs (C# Repl)",
	"version": "0.0.1",
	"publisher": "filipw",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.scriptcsRepl"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.scriptcsRepl",
			"title": "Execute with scriptcs"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}